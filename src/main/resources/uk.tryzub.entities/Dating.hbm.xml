<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="uk.tryzub.entities.Dating" table="dating" schema="tryzub">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="section">
            <column name="section" sql-type="int(11)"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="city">
            <column name="city" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="message">
            <column name="message" sql-type="varchar(1000)" length="1000"/>
        </property>
    </class>
</hibernate-mapping>